package com.sapient.pe.collections;

import java.util.ArrayList;
import java.util.Iterator;
import java.util.List;
import java.util.ListIterator;

import com.sapient.pe.tutorial.Employee;

public class ArrayListDemo {
	public static void main(String[] args) {
		List<Integer> values = new ArrayList<Integer>();
		values.add(56);
		values.add(49);
		values.add(47);
		System.out.println(values.size());
		values.add(2,34);
		
		List<Integer> newList = new ArrayList<Integer>();
		newList.addAll(values);
		newList.remove(0);
		System.out.println("size of new List "+newList.size());
		System.out.println("size of values "+values.size());
		Employee e = new Employee(21,"Divya",500000);
		for(int index=0;index<newList.size();index++) {
			System.out.println(newList.get(index));
		}
		for(Integer val: newList) {
			
		}
		Iterator<Integer> it = newList.iterator();
		while(it.hasNext()) {
			Integer val = it.next();
			System.out.println("values is: "+val);
		}
		ListIterator<Integer> it1 = newList.listIterator();
		while(it1.hasNext()) {
			Integer val = it1.next();
			System.out.println("values is: "+val);
		}
		CollectionDemo c = new CollectionDemo();
	}
}
