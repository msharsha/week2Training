package com.sapient.pe.collections;

import java.util.Comparator;

import com.sapient.pe.tutorial.BankAccount;
import com.sapient.pe.tutorial.Employee;

public class CustomComparators{

	

}

class EmployeeIdDESCComparator implements Comparator<Employee>{

	@Override
	public int compare(BankAccount emp1, BankAccount emp2) {
		// TODO Auto-generated method stub
		return Integer.valueOf(emp1.getId()).compareTo(emp2.getId());
	}

}

class EmployeeNameDESCComparator implements Comparator<Employee>{

	@Override
	public int compare(Employee emp1, Employee emp2) {
		// TODO Auto-generated method stub
		return emp2.getName().compareTo(emp1.getName());
	}

}
class EmployeeNameAscComparator implements Comparator<Employee>{

	@Override
	public int compare(Employee emp1, Employee emp2) {
		// TODO Auto-generated method stub
		 return emp1.getName().compareTo(emp2.getName());
	}

}
