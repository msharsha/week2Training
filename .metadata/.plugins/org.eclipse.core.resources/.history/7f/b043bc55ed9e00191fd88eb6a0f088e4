package com.sapient.pe.collections;

import java.util.Comparator;

import com.sapient.pe.tutorial.BankAccount;
import com.sapient.pe.tutorial.Employee;

public class CustomComparators{

	

}

class AccountIdDESCComparator implements Comparator<BankAccount>{

	@Override
	public int compare(BankAccount emp1, BankAccount emp2) {
		// TODO Auto-generated method stub
		return Long.valueOf(emp1.getAccountNo()).compareTo(emp2.getAccountNo());
	}

}

class EmployeeNameDESCComparator implements Comparator<BankAccount>{

	@Override
	public int compare(BankAccount emp1, BankAccount emp2) {
		// TODO Auto-generated method stub
		return emp2.getPerson().getName().compareTo(emp1.getPerson().getName());
	}

}
class EmployeeNameAscComparator implements Comparator<BankAccount>{

	@Override
	public int compare(BankAccount emp1, BankAccount emp2) {
		// TODO Auto-generated method stub
		 return emp1.getPerson().getName().compareTo(emp2.getPerson().getName());
	}

}
